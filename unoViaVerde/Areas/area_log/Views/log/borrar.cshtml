
@{
    ViewBag.Title = "borrar";
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Activadores</title>
    <link href="~/Content/estilo/style_dos.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>

<body>
    <h3>Eliminar datos</h3>
    <div class="jumbo">
        <h3> @ViewBag.mensaje</h3>
        <form action="~/area_log/log/borrar" method="post">
            <div class="row">
                <div class="col-md-12">
                    <span>Rut:</span>
                    <input type="text" name="rut" value="" />
                    <input type="submit" class="btn" value="eliminar" />
                </div>
            </div>
        </form>
        <div class="row">
            <div class="col-md-4">
                <form method="post" action="~/area_log/log/admin">
                    <input type="hidden" name="op" value="editar">
                    <input type="submit" class="btn" value="EDITAR">
                </form>
            </div>
            <div class="col-md-4">
                <form method="post" action="~/area_log/log/admin">
                    <input type="hidden" name="op" value="mostrar">
                    <input type="submit" class="btn" value="Mostrar">
                </form>
            </div>
        </div>

    </div>

</body>
</html>





