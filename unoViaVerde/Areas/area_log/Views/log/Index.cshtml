
@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Activadores</title>
    <link href="~/Content/estilo/style_dos.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>

<body>



<div class="  jumbo">
    <div class="row">
        <div class="col-md-3 ">

        </div>
        <div class="col-md-6 ">
         
            <form action="~/area_log/log/buscar" method="post">
                <div class="form-group ">
                    <label for="inputPassword3" class=" col-form-label text-white">Rut</label>
                    <input type="text" name="rut" class="form-control" />
                </div>
                <div class="form-group ">
                    <label for="inputPassword3" class=" col-form-label text-white">Ingrese Password</label>
                    <input type="password" name="pass" class="form-control" id="inputPassword3" value="@ViewData["pass"]" placeholder="Password" />

                </div>
                <div class="form-group ">

                    <input type="hidden" name="nom" class="form-control"  value="@ViewData["nom"]" />
                    <input type="hidden" name="clasi" class="form-control" value="@ViewData["clasi"]" />
                    <input type="hidden" name="categoria" class="form-control" value="@ViewData["categoria"]" />
                    <input type="hidden" name="dir" class="form-control" value="@ViewData["dir"]" />
                </div>
              
                <div class="form-group row">
                    <div class="col-sm-10">
                        <button type="submit" class="btn btn-primary">Enviar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

</body>
</html>


